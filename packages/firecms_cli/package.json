{
  "name": "@firecms/cli",
  "version": "3.0.0-beta.3",
  "description": "A CLI to perform FireCMS operations",
  "packageManager": "yarn@4.1.0",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "source": "src/index.ts",
  "bin": {
    "firecms": "bin/firecms"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf templates/template_v3/node_modules && rm -rf templates/template_v3/yarn.lock && tsc",
    "dev": "ts-node src/index.ts",
    "clean": "rm -rf dist && find ./src -name '*.js' -type f | xargs rm -f"
  },
  "keywords": [
    "cli",
    "create-firecms-app",
    "firecms",
    "firebase",
    "firestore",
    "cms",
    "react",
    "typescript"
  ],
  "author": "firecms.co",
  "license": "MIT",
  "dependencies": {
    "arg": "^5.0.2",
    "axios": "^0.28.0",
    "chalk": "^4.1.2",
    "execa": "^4.1.0",
    "form-data": "^4.0.0",
    "googleapis": "^133.0.0",
    "inquirer": "^7.3.3",
    "json5": "^2.2.3",
    "listr": "^0.14.3",
    "ncp": "^2.0.0",
    "nocache": "^4.0.0",
    "open": "^7.4.2",
    "ora": "^5.4.1",
    "ws": "^8.16.0",
    "zip-folder": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "files": [
    "bin/",
    "build/",
    "src/",
    "templates/"
  ],
  "exclude": [
    "node_modules",
    "template/node_modules"
  ],
  "gitHead": "33cceff5d5bc2a7002cc5a3904b787e70bfb9c6e"
}
